<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Saved">

    <?xml version="1.0" encoding="utf-8"?>
    <androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Cart">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            app:layout_editor_absoluteX="1dp"
            app:layout_editor_absoluteY="1dp"
            >

            <TextView
                android:id="@+id/textView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="40dp"
                android:fontFamily="@font/alegreya_sans_extrabold"
                android:text="JRSL."
                android:textAppearance="@style/TextAppearance.AppCompat.Display2"
                android:textColor="#000"
                android:textSize="40sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:text="SAVED (4)" />

            <TextView
                android:id="@+id/all"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:fontFamily="@font/alegreya_sans_bold"
                android:textAppearance="@style/TextAppearance.AppCompat.Display2"
                android:textColor="#000"
                android:textSize="35sp"
                android:layout_gravity="center"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:text="ALL" />

            <androidx.cardview.widget.CardView
                android:id="@+id/card_view1"
                android:layout_width="180dp"
                android:layout_height="320dp"
                android:layout_gravity="left"
                android:layout_marginTop="20dp"
                app:cardBackgroundColor="@android:color/transparent"
                app:cardElevation="0.5dp">
                <ImageView
                    android:id="@+id/close1"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_gravity="right"
                    android:src="@drawable/ic_baseline_close_24" />
                <ImageView
                    android:id="@+id/orderPicture1"
                    android:layout_width="180sp"
                    android:padding="5dp"
                    android:layout_height="240sp"
                    android:layout_gravity="center"
                    android:layout_marginBottom="20dp"
                    android:src="@drawable/outfit1" />

                <TextView
                    android:id="@+id/Saved1Brand"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom"
                    android:padding="5dp"
                    android:fontFamily="@font/alegreya_sans_bold"
                    android:text="@string/Saved1Brand"
                    android:textColor="@color/black"
                    android:layout_marginBottom="20dp"/>

                <TextView
                    android:id="@+id/Saved1Desc"
                    android:layout_width="match_parent"
                    android:layout_height="47dp"
                    android:layout_gravity="bottom"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="18dp"
                    android:fontFamily="@font/alegreya_sans_medium"
                    android:padding="5dp"
                    android:text="@string/Saved1Desc"
                    android:textColor="@color/black"
                    android:textSize="13dp" />

                <TextView
                    android:id="@+id/Saved1Price"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:layout_gravity="bottom"
                    android:padding="5dp"
                    android:textSize="15dp"
                    android:fontFamily="@font/alegreya_sans_bold"
                    android:text="@string/Saved1Price"
                    android:textColor="@color/black"
                    android:layout_marginTop="5dp"
                    />

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/card_view3"
                android:layout_width="180dp"
                android:layout_height="320dp"
                android:layout_gravity="left"
                android:layout_marginTop="20dp"
                app:cardBackgroundColor="@android:color/transparent"
                app:cardElevation="0.5dp">
                <ImageView
                    android:id="@+id/close3"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_gravity="right"
                    android:src="@drawable/ic_baseline_close_24" />
                <ImageView
                    android:id="@+id/orderPicture3"
                    android:layout_width="180sp"
                    android:padding="5dp"
                    android:layout_height="240sp"
                    android:layout_gravity="center"
                    android:layout_marginBottom="20dp"
                    android:src="@drawable/outfit3" />

                <TextView
                    android:id="@+id/Saved3Brand"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom"
                    android:padding="5dp"
                    android:fontFamily="@font/alegreya_sans_bold"
                    android:text="@string/Saved3Brand"
                    android:textColor="@color/black"
                    android:layout_marginBottom="20dp"/>

                <TextView
                    android:id="@+id/Saved3Desc"
                    android:layout_width="match_parent"
                    android:layout_height="47dp"
                    android:layout_gravity="bottom"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="18dp"
                    android:fontFamily="@font/alegreya_sans_medium"
                    android:padding="5dp"
                    android:text="@string/Saved3Desc"
                    android:textColor="@color/black"
                    android:textSize="13dp" />
                <TextView
                    android:id="@+id/Saved3Price"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:layout_gravity="bottom"
                    android:padding="5dp"
                    android:textSize="15dp"
                    android:fontFamily="@font/alegreya_sans_bold"
                    android:text="@string/Saved3Price"
                    android:textColor="@color/black"
                    android:layout_marginTop="5dp"
                    />

            </androidx.cardview.widget.CardView>


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            app:layout_editor_absoluteX="1dp"
            app:layout_editor_absoluteY="1dp">

            <androidx.cardview.widget.CardView
                android:id="@+id/card_view2"
                android:layout_width="180dp"
                android:layout_height="320dp"
                android:layout_gravity="left"
                android:layout_marginTop="170dp"
                android:layout_marginLeft="210dp"
                app:cardBackgroundColor="@android:color/transparent"
                app:cardElevation="0.5dp">
                <ImageView
                    android:id="@+id/close2"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_gravity="right"
                    android:src="@drawable/ic_baseline_close_24" />
                <ImageView
                    android:id="@+id/orderPicture2"
                    android:layout_width="180sp"
                    android:padding="5dp"
                    android:layout_height="240sp"
                    android:layout_gravity="center"
                    android:layout_marginBottom="20dp"
                    android:src="@drawable/outfit2" />

                <TextView
                    android:id="@+id/Saved2Brand"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom"
                    android:padding="5dp"
                    android:fontFamily="@font/alegreya_sans_bold"
                    android:text="@string/Saved2Brand"
                    android:textColor="@color/black"
                    android:layout_marginBottom="20dp"/>

                <TextView
                    android:id="@+id/Saved2Desc"
                    android:layout_width="match_parent"
                    android:layout_height="47dp"
                    android:layout_gravity="bottom"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="20dp"
                    android:fontFamily="@font/alegreya_sans_medium"
                    android:padding="7dp"
                    android:text="@string/Saved2Desc"
                    android:textColor="@color/black"
                    android:textSize="13dp" />

                <TextView
                    android:id="@+id/Saved2Price"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:layout_gravity="bottom"
                    android:padding="5dp"
                    android:textSize="15dp"
                    android:fontFamily="@font/alegreya_sans_bold"
                    android:text="@string/Saved2Price"
                    android:textColor="@color/black"
                    android:layout_marginTop="5dp"
                    />

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/card_view4"
                android:layout_width="180dp"
                android:layout_height="320dp"
                android:layout_gravity="left"
                android:layout_marginTop="20dp"
                android:layout_marginLeft="210dp"
                app:cardBackgroundColor="@android:color/transparent"
                app:cardElevation="0.5dp">
                <ImageView
                    android:id="@+id/close4"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_gravity="right"
                    android:src="@drawable/ic_baseline_close_24" />
                <ImageView
                    android:id="@+id/orderPicture4"
                    android:layout_width="180sp"
                    android:padding="5dp"
                    android:layout_height="240sp"
                    android:layout_gravity="center"
                    android:layout_marginBottom="29dp"
                    android:src="@drawable/outfit4" />

                <TextView
                    android:id="@+id/Saved4Brand"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom"
                    android:padding="5dp"
                    android:fontFamily="@font/alegreya_sans_bold"
                    android:text="@string/Saved4Brand"
                    android:textColor="@color/black"
                    android:layout_marginBottom="20dp"/>

                <TextView
                    android:id="@+id/Saved4Desc"
                    android:layout_width="match_parent"
                    android:layout_height="47dp"
                    android:layout_gravity="bottom"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="18dp"
                    android:fontFamily="@font/alegreya_sans_medium"
                    android:padding="5dp"
                    android:text="@string/Saved4Desc"
                    android:textColor="@color/black"
                    android:textSize="13dp" />
                <TextView
                    android:id="@+id/Saved4Price"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:layout_gravity="bottom"
                    android:padding="5dp"
                    android:textSize="15dp"
                    android:fontFamily="@font/alegreya_sans_bold"
                    android:text="@string/Saved4Price"
                    android:textColor="@color/black"
                    android:layout_marginTop="5dp"
                    />

            </androidx.cardview.widget.CardView>


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            app:layout_editor_absoluteX="1dp"
            app:layout_editor_absoluteY="1dp"
            >
            <ImageView
                android:id="@+id/imageViewCart"
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:layout_marginTop="34dp"
                android:layout_marginStart="355dp"
                android:contentDescription="Cart Image"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.541"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/cart" />



        </LinearLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>